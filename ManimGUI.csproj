<Project Sdk="Godot.NET.Sdk/4.2.2">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <TargetFramework Condition=" '$(GodotTargetPlatform)' == 'android' ">net7.0</TargetFramework>
    <TargetFramework Condition=" '$(GodotTargetPlatform)' == 'ios' ">net8.0</TargetFramework>
    <EnableDynamicLoading>true</EnableDynamicLoading>
  </PropertyGroup>
  <ItemGroup>
    <Folder Include="Assets\Scenes\" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Assets\References\BetterNumberSystem.dll" />
    <None Include="Assets\References\ManimCSharp.dll" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="BetterNumberSystem">
      <HintPath>Assets\References\BetterNumberSystem.dll</HintPath>
    </Reference>
    <Reference Include="ManimCSharp">
      <HintPath>Assets\References\ManimCSharp.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>